prog$fib:
do;
    declare val integer initial (7);

    fib: procedure(n) integer;
        declare n integer;
        if n = 0 then
            return 0;
        else if n = 1 then
            return 1;
        else
            return fib(n - 1) + fib(n - 2);
    end fib;

    call writeln(fib(val));
end prog$fib;

